<div class="container1" id="body-user">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>



    <div class="row">
        <div class="col-md-5 col-md-offset-6">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <span class="glyphicon glyphicon-lock" style="font-size: 12px;"></span> Log In</div>
                <div class="panel-body">
                    <form class="form-horizontal" role="form">
                   
                        <div class="form-group">
                          <div class="col-md-3">
                          <%= f.label :email %>
                          </div>
                          <div class="col-md-9">
                          <%= f.email_field :email, autofocus: true, placeholder: "Enter Email", :class => "form-control" %>
                          </div>
                        </div>

                        <div class="form-group">
                          <div class="col-md-3">
                            <%= f.label :password %>
                            
                            
                            
                          </div>
                          <div class="col-md-9">
                            <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Enter Password" %>
                          </div>
                        </div>

                        <% if devise_mapping.rememberable? -%>
                        <div class="form-group">
                          <div class="col-md-6 col-md-offset-3">
                            <%= f.check_box :remember_me %>
                            <%= f.label :remember_me %>
                          </div>
                          
                        <% end %>
                          
                        </div>
  
                        <div class="form-group">
                          <div class="col-md-3">
                             
                          </div>
                          <div class="col-md-9">
                            <div class="actions">
                                <%= f.submit "Log In", :class => 'btn btn-success' %>
                              </div>

                          </div>
                        </div>
    

                    </form>
                </div>
                <div class="panel-footer">
                    <%= render "devise/shared/links" %></div>
                    <% end %>
            </div>
        </div>
    </div>
</div>




